        <BoxPane styles="{padding:4, horizontalAlignment:'center', verticalAlignment:'center'}">
            <PushButton bxml:id="pushButton" buttonData="Click Me!"/>
        </BoxPane>
    </Border>
</buttons:PushButtonWindow>
